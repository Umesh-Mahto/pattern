//Ques 15 :

public class Main
{
	public static void main(String[] args) {
		int n=5;
		int limit=n;
		int space=0;
		for(int i=0;i<n*2-1;i++){
		    for(int j=1;j<=space;j++){
		        System.out.print("  ");
		    }
		    for(int j=0;j<limit;j++){
		        System.out.print("* ");
		    }
		    if(i<=n/2+1){
		        limit--;
		    space+=2;
		    }
		    else{
		        limit++;
		    space-=2;
		    }
		    System.out.println();
		}
	}
}
