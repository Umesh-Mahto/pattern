//Ques 11 :

public class Main
{
	public static void main(String[] args) {
		int n=5;
		int limit=1;
		int space=n-1;
		for(int i=0;i<n;i++){
		    for(int j=0;j<space;j++){
		        System.out.print(" ");
		    }
		    for(int j=0;j<limit;j++){
		        System.out.print("* ");
		    }
		    limit++;
		    space--;
		    System.out.println();
		}
	}
}
