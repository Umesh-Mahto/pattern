//ques 16:

public class Main
{
	public static void main(String[] args) {
	int n=5;
	 int space=n-1;
	 int star=n;
	 
	 for(int i=0;i<n*2-1;i++){
	     
	     for(int j=0;j<space;j++){
	         System.out.print("   ");
	     }
	     
	     for(int j=0;j<star;j++){
	         System.out.print("* ");
	     }
	     if(i<=n/2+1){
	       space--;
	     star--;  
	     }else{
	         space++;
	     star++;
	     }
	    
	     System.out.println();
	 }
	}
}
