//Ques 6 :

public class Main
{
	public static void main(String[] args) {
		int n=5;
		int limit=n;
		int space=0;
		for(int i=0;i<n;i++){
		    for(int j=0;j<space;j++){
		        System.out.print("  ");
		    }
		    for(int j=0;j<limit;j++){
		        System.out.print("* ");
		    }
		    limit--;
		    space+=2;
		    System.out.println();
		}
	}
}
