//Ques 12 :

public class Main
{
	public static void main(String[] args) {
		int n=5;
		int limit=1;
		int space=0;
		for(int i=0;i<n*2-1;i++){
		    
		    for(int j=0;j<limit;j++){
		        System.out.print("* ");
		    }
		    if(i<=n/2+1){
		      limit++;  
		    }
		    else{
		        limit--;
		    }
		    System.out.println();
		}
	}
}
